{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "mim-webapp",
  "compatibility_date": "2025-09-28",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "mim-production",
      "database_id": "YOUR-ACTUAL-D1-DATABASE-ID-HERE"
    }
  ]
}